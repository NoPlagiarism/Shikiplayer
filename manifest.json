{

  "manifest_version": 2,
  "name": "Shikiplayer",
  "version": "2.1.0",

  "description": "Adds Kodik player to shikimori.one website.",
  "icons": {
    "128": "./media/icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://shikimori.one/*"],
      "js": ["./scripts/helpers/helpers.js",
             "./scripts/reloader.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["*://shikimori.one/*"],
      "js": ["./config.js", "./scripts/helpers/helpers.js", "./scripts/helpers/shikimori.js",
             "./scripts/player.js"],
      "run_at": "document_end"
    }
  ]

}