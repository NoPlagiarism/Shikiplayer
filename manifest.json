{

  "manifest_version": 3,
  "name": "Shikiplayer",
  "version": "3.0.3",

  "description": "Adds Kodik player to shikimori.me website.",
  "icons": {
    "128": "./images/icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://shikimori.me/*"],
      "js": [
        "./config.js",

        "./scripts/helpers/helpers.js",
        "./scripts/helpers/shikimori.js",

        "./scripts/player.js"
        ],
      "run_at": "document_start"
    }
  ]
}
