{

  "manifest_version": 2,
  "name": "Shikiplayer",
  "version": "1.0",

  "description": "Adds Kodik player to shikimori.one website.",
  "icons": {
    "1024": "icons/icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://shikimori.one/*"],
      "js": ["shikiplayer.js"]
    }
  ]

}